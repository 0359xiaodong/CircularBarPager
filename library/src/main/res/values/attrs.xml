<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularBar">
        <attr name="arc_progress" format="integer"/>
        <attr name="progress_arc_max" format="integer"/>

        <attr name="progress_arc_unreached_color" format="color"/>
        <attr name="progress_arc_reached_color" format="color"/>

        <attr name="progress_arc_reached_width" format="dimension"/>
        <attr name="progress_arc_unreached_width" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="CircularViewPager">
        <attr name="progress_pager_on_click_enabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="CircularBarPagerStyle" format="reference"/>
    </declare-styleable>
</resources>